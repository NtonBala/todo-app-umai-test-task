        "babel-loader": "~7.1.5",
        "chalk": "2.4.1",
        "clean-webpack-plugin": "~0.1.19",
        "cross-env": "~5.2.0",
        "css-loader": "1.0.0",
        "css-mqpacker": "~7.0.0",
        "cssnano": "~4.0.4",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.1",
        "enzyme-to-json": "~3.3.4",
        "eslint": "5.2.0",
        "eslint-plugin-react": "~7.10.0",
